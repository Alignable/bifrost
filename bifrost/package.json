{
  "name": "@alignable/bifrost",
  "repository": "https://github.com/Alignable/bifrost.git",
  "version": "0.1.0",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    "./config": {
      "types": "./dist/renderer/config.d.ts",
      "default": "./dist/renderer/+config.js"
    },
    "./__internal/renderer/*": {
      "types": "./dist/renderer/*.d.ts",
      "default": "./dist/renderer/*.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "config": [
        "./dist/renderer/config.d.ts"
      ],
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "release": "release-me patch",
    "build": "tsup",
    "tsc": "tsc"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "tough-cookie": "^4.1.2",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "typescript": ">=4.7",
    "vike": ">=0.4.244",
    "vike-react": ">=0.6.11"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.2",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@vitejs/plugin-react": "^4.7.0",
    "react": "^19.2.0",
    "tsup": "^7.1.0",
    "turbolinks": "5.3.0-beta.1",
    "typescript": "^5.0.4",
    "vike-react": "0.6.11",
    "vite": "^6.3.5",
    "vike": "0.4.244",
    "cross-env": "^7.0.3"
  }
}
